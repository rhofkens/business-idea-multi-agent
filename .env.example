OPENAI_API_KEY="your_api_key_here"

# Model configuration (optional, defaults shown)
# Global model configuration - applies to all agents unless overridden
LLM_MODEL="o3"             # Options: "o3", "gpt-4o" - Default model for all agents

# Per-agent model configuration (optional, overrides LLM_MODEL)
# Leave commented to use the global LLM_MODEL setting
# IDEATION_MODEL="o3"        # Model for ideation agent
# COMPETITOR_MODEL="o3"      # Model for competitor analysis agent
# CRITIC_MODEL="o3"          # Model for business critic agent
# DOCUMENTATION_MODEL="o3"   # Model for documentation agent

USE_REFINEMENT="true"      # Options: "true", "false" - Enable two-pass refinement for ideation

# Server configuration
NODE_ENV="development"     # Options: "development", "production", "test"
PORT="3001"               # Port for the backend server

# Authentication configuration
SESSION_SECRET="your-session-secret-min-32-chars"  # Must be at least 32 characters in production
CORS_ORIGIN=""            # Optional: Custom CORS origin for production (e.g., "https://yourdomain.com")